{
  "name": "@datacamp/waffles-tag",
  "version": "1.2.4",
  "description": "Creates a small tag element for things like XP points.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/datacamp-engineering/design-system/tree/master/packages/react-components/tag/"
  },
  "scripts": {
    "build:cjs": "BABEL_ENV=cjs babel src --extensions '.tsx','.ts' --out-dir lib --ignore src/**/*.spec.tsx --config-file ./babel7.config.js",
    "build:es": "BABEL_ENV=es babel src --extensions '.tsx','.ts' --out-dir es --ignore src/**/*.spec.tsx --config-file ./babel7.config.js",
    "build:legacy": "copyfiles -u 1 legacy/*.scss lib",
    "clean": "rm -rf lib es types componentMetadata.json",
    "build:types": "tsc --rootDir src --declarationDir types --emitDeclarationOnly",
    "build:metadata": "react-docgen src --exclude \"/^.+spec.+$/\" -x tsx -o componentMetadata.json --resolver findAllComponentDefinitions",
    "build": "yarn build:types && yarn build:es && yarn build:cjs && yarn build:legacy && yarn build:metadata",
    "prepare": "yarn clean && yarn build",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "watch": "yarn build:es --watch",
    "stryker": "stryker run"
  },
  "keywords": [],
  "author": "DataCamp",
  "contributors": [
    "Jordan Wade <jjwade31@gmail.com>"
  ],
  "license": "ISC",
  "directories": {
    "src": "src"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@datacamp/waffles-axe-render": "^1.2.1",
    "@datacamp/waffles-babel-preset": "^1.4.1",
    "@datacamp/waffles-tsconfig": "^1.1.8",
    "@emotion/core": "^10.0.17",
    "@stryker-mutator/core": "^2.5.0",
    "@stryker-mutator/html-reporter": "^2.5.0",
    "@stryker-mutator/typescript": "^2.4.0",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "9.4.1",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.9",
    "babel-jest": "^25.1.0",
    "copyfiles": "^2.1.1",
    "jest": "24.9.0",
    "jest-emotion": "^10.0.26",
    "react": "^16.6.3",
    "react-docgen": "5.2.1",
    "react-dom": "^16.6.3",
    "typescript": "3.8.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "@datacamp/waffles-core": "^1.2.29",
    "@datacamp/waffles-text": "^2.2.4",
    "@datacamp/waffles-tokens": "^0.6.8",
    "lodash": "^4.17.13",
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.17",
    "react": ">= 16.6.3"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "gitHead": "033030174dbf18623ccd9c0174108c41a9e588a1"
}
