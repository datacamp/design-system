{
  "name": "@datacamp/waffles-text",
  "version": "0.5.1",
  "main": "lib/index.js",
  "license": "UNLICENSED",
  "sideEffects": [
    "lib/sideEffects/*"
  ],
  "scripts": {
    "build:js": "babel src --extensions '.tsx','.ts' --out-dir lib --ignore \"src/**/*.spec.tsx\",\"src/**/*.spec.ts\" --config-file ./babel7.config.js",
    "clean": "rm -rf lib",
    "build:types": "tsc --rootDir src --declarationDir lib --emitDeclarationOnly",
    "build": "yarn build:types && yarn build:js",
    "prepare": " yarn clean && yarn build",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "watch": "yarn build:js --watch"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.0",
    "@datacamp/waffles-babel-preset": "^1.0.5",
    "@datacamp/waffles-tsconfig": "1.0.1",
    "@testing-library/react": "8.0.4",
    "@types/lodash": "4.14.134",
    "babel-jest": "24.6.0",
    "jest": "24.6.0",
    "jest-dom": "3.5.0",
    "jest-emotion": "10.0.14",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "dependencies": {
    "@datacamp/waffles-tokens": "^0.5.1",
    "@emotion/core": "^10.0.14",
    "emotion": "^10.0.14",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
