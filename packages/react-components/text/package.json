{
  "name": "@datacamp/waffles-text",
  "version": "2.2.3",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "license": "UNLICENSED",
  "sideEffects": [
    "lib/sideEffects/*",
    "es/sideEffects/*"
  ],
  "scripts": {
    "build:cjs": "BABEL_ENV=cjs babel src --extensions '.tsx','.ts' --out-dir lib --ignore \"src/**/*.spec.tsx\",\"src/**/*.spec.ts\" --config-file ./babel7.config.js",
    "build:es": "BABEL_ENV=es babel src --extensions '.tsx','.ts' --out-dir es --ignore \"src/**/*.spec.tsx\",\"src/**/*.spec.ts\" --config-file ./babel7.config.js",
    "clean": "rm -rf lib es types componentMetadata.json",
    "build:types": "tsc --rootDir src --declarationDir types --emitDeclarationOnly",
    "build:metadata": "react-docgen src --exclude \"/^.+spec.+$/\" -x tsx -o componentMetadata.json --resolver findAllComponentDefinitions",
    "build": "yarn build:types && yarn build:cjs && yarn build:es && yarn build:metadata",
    "prepare": " yarn clean && yarn build",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "watch": "yarn build:es --watch",
    "stryker": "stryker run"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/core": "7.7.7",
    "@datacamp/waffles-axe-render": "^1.2.0",
    "@datacamp/waffles-babel-preset": "^1.4.0",
    "@datacamp/waffles-tsconfig": "^1.1.7",
    "@emotion/core": "^10.0.17",
    "@stryker-mutator/core": "^2.5.0",
    "@stryker-mutator/html-reporter": "^2.5.0",
    "@stryker-mutator/typescript": "^2.4.0",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "9.4.1",
    "@types/lodash": "4.14.149",
    "babel-jest": "25.1.0",
    "jest": "24.9.0",
    "jest-emotion": "10.0.26",
    "react": "16.8.6",
    "react-docgen": "5.2.1",
    "react-dom": "16.8.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "@datacamp/waffles-tokens": "^0.6.8",
    "@datacamp/waffles-utils": "^1.3.3",
    "lodash": "^4.17.13",
    "prop-types": "^15.7.2",
    "tinycolor2": "^1.4.1"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.17",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
