{
  "name": "@datacamp/waffles-text",
  "version": "4.0.1",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "files": [
    "es",
    "lib",
    "types"
  ],
  "license": "UNLICENSED",
  "sideEffects": [
    "lib/sideEffects/*",
    "es/sideEffects/*"
  ],
  "scripts": {
    "build:cjs": "BABEL_ENV=cjs babel src --extensions '.tsx','.ts' --out-dir lib --ignore \"src/**/*.spec.tsx\",\"src/**/*.spec.ts\" --config-file ./babel7.config.js",
    "build:es": "BABEL_ENV=es babel src --extensions '.tsx','.ts' --out-dir es --ignore \"src/**/*.spec.tsx\",\"src/**/*.spec.ts\" --config-file ./babel7.config.js",
    "clean": "rm -rf lib es types componentMetadata.json",
    "build:types": "tsc --rootDir src --declarationDir types --emitDeclarationOnly",
    "build:metadata": "react-docgen src --exclude \"/^.+spec.+$/\" -x tsx -o componentMetadata.json --resolver findAllComponentDefinitions",
    "build": "yarn build:types && yarn build:cjs && yarn build:es && yarn build:metadata",
    "prepare": " yarn clean && yarn build",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "watch": "yarn build:es --watch",
    "stryker": "stryker run"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/core": "7.12.3",
    "@datacamp/waffles-axe-render": "^1.2.4",
    "@datacamp/waffles-babel-preset": "^2.0.1",
    "@datacamp/waffles-tsconfig": "^1.1.12",
    "@emotion/core": "^10.0.17",
    "@stryker-mutator/core": "^3.3.1",
    "@stryker-mutator/html-reporter": "^3.1.0",
    "@stryker-mutator/typescript": "^3.3.1",
    "@testing-library/jest-dom": "5.11.3",
    "@testing-library/react": "9.4.1",
    "@types/lodash": "4.14.157",
    "babel-jest": "26.2.2",
    "jest": "26.6.1",
    "jest-emotion": "10.0.32",
    "react": "16.8.6",
    "react-docgen": "5.3.1",
    "react-dom": "16.8.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "@datacamp/waffles-tokens": "^1.0.0",
    "@datacamp/waffles-utils": "^2.0.18",
    "airbnb-prop-types": "^2.16.0",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react-uid": "^2.3.0",
    "tinycolor2": "^1.4.1"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.17",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
