{
  "name": "@datacamp/waffles-text",
  "version": "1.2.1",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "license": "UNLICENSED",
  "sideEffects": [
    "lib/sideEffects/*",
    "es/sideEffects/*"
  ],
  "scripts": {
    "build:cjs": "BABEL_ENV=cjs babel src --extensions '.tsx','.ts' --out-dir lib --ignore \"src/**/*.spec.tsx\",\"src/**/*.spec.ts\" --config-file ./babel7.config.js",
    "build:es": "BABEL_ENV=es babel src --extensions '.tsx','.ts' --out-dir es --ignore \"src/**/*.spec.tsx\",\"src/**/*.spec.ts\" --config-file ./babel7.config.js",
    "clean": "rm -rf lib es types componentMetadata.json",
    "build:types": "tsc --rootDir src --declarationDir types --emitDeclarationOnly",
    "build:metadata": "react-docgen src --exclude \"/^.+spec.+$/\" -x tsx -o componentMetadata.json --resolver findAllComponentDefinitions",
    "build": "yarn build:types && yarn build:cjs && yarn build:es && yarn build:metadata",
    "prepare": " yarn clean && yarn build",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "watch": "yarn build:es --watch",
    "stryker": "stryker run"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.6.4",
    "@datacamp/waffles-axe-render": "^1.1.4",
    "@datacamp/waffles-babel-preset": "^1.3.5",
    "@datacamp/waffles-tsconfig": "^1.1.4",
    "@emotion/core": "^10.0.17",
    "@stryker-mutator/core": "^2.0.2",
    "@stryker-mutator/html-reporter": "^2.0.2",
    "@stryker-mutator/jest-runner": "^2.0.2",
    "@stryker-mutator/typescript": "^2.0.2",
    "@testing-library/jest-dom": "4.2.3",
    "@testing-library/react": "8.0.7",
    "@types/jest": "24.0.21",
    "@types/lodash": "4.14.144",
    "babel-jest": "24.9.0",
    "jest": "24.9.0",
    "jest-emotion": "10.0.17",
    "react": "16.8.6",
    "react-docgen": "5.0.0-beta.1",
    "react-dom": "16.8.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "@datacamp/waffles-tokens": "^0.6.6",
    "@datacamp/waffles-utils": "^1.2.4",
    "emotion": "^10.0.17",
    "lodash": "^4.17.13",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.17",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
