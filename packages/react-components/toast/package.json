{
  "name": "@datacamp/waffles-toast",
  "version": "4.0.0-beta.6",
  "description": "The Waffles Toast",
  "keywords": [
    "toast"
  ],
  "homepage": "https://github.com/datacamp/design-system/tree/master/packages/react-components/toast/README.md",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "files": [
    "es",
    "lib",
    "types"
  ],
  "scripts": {
    "build:cjs": "BABEL_ENV=cjs babel src --extensions '.tsx','.ts' --out-dir lib --ignore \"src/**/*.spec.tsx\",\"src/**/*.spec.ts\" --config-file ./babel7.config.js",
    "build:es": "BABEL_ENV=es babel src --extensions '.tsx','.ts' --out-dir es --ignore \"src/**/*.spec.tsx\",\"src/**/*.spec.ts\" --config-file ./babel7.config.js",
    "build:legacy": "copyfiles -u 1 legacy/*.scss lib",
    "build:metadata": "react-docgen src --exclude \"/^.+spec.+$/\" -x tsx -o componentMetadata.json --resolver findAllComponentDefinitions",
    "clean": "rm -rf lib es types componentMetadata.json",
    "build:types": "tsc --rootDir src --declarationDir types --emitDeclarationOnly",
    "build": "yarn build:types && yarn build:cjs && yarn build:es && yarn build:legacy && yarn build:metadata",
    "prepare": " yarn clean && yarn build",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "watch": "yarn build:es --watch",
    "stryker": "stryker run"
  },
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.12.7",
    "@datacamp/waffles-axe-render": "^1.2.8",
    "@datacamp/waffles-babel-preset": "^3.1.0",
    "@datacamp/waffles-icons": "^5.0.0-beta.6",
    "@datacamp/waffles-tsconfig": "^2.0.0",
    "@emotion/jest": "11.2.0",
    "@emotion/react": "11.1.4",
    "@stryker-mutator/core": "4.3.1",
    "@stryker-mutator/html-reporter": "3.1.0",
    "@stryker-mutator/typescript": "4.0.0",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@types/airbnb-prop-types": "2.13.1",
    "babel-jest": "26.6.3",
    "jest": "26.6.1",
    "react": "17.0.1",
    "react-docgen": "5.3.1",
    "react-dom": "17.0.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "@datacamp/waffles-button": "^8.0.0-beta.6",
    "@datacamp/waffles-text": "^6.0.0-beta.4",
    "@datacamp/waffles-tokens": "^2.0.0-beta.5",
    "@datacamp/waffles-utils": "^3.3.1",
    "prop-types": "^15.7.2",
    "react-toastify": "^6.1.0"
  },
  "peerDependencies": {
    "@datacamp/waffles-icons": "^1.3.3",
    "@emotion/react": "^11.0.0",
    "react": "^16.8.6 || ^17.0.1",
    "react-dom": "^16.8.6 || ^17.0.1"
  },
  "bugs": {
    "url": "https://github.com/datacamp/design-system/issues"
  },
  "gitHead": "033030174dbf18623ccd9c0174108c41a9e588a1"
}
