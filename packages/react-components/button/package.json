{
  "name": "@datacamp/waffles-button",
  "version": "2.0.0",
  "description": "The Waffles Button",
  "keywords": [
    "button"
  ],
  "author": "Dan Denney <dd.subscriptions@gmail.com>",
  "homepage": "https://github.com/datacamp/design-system/tree/master/packages/button/README.md",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:cjs": "BABEL_ENV=cjs babel src --extensions '.tsx','.ts' --out-dir lib --ignore \"src/**/*.spec.tsx\",\"src/**/*.spec.ts\" --config-file ./babel7.config.js",
    "build:es": "BABEL_ENV=es babel src --extensions '.tsx','.ts' --out-dir es --ignore \"src/**/*.spec.tsx\",\"src/**/*.spec.ts\" --config-file ./babel7.config.js",
    "build:legacy": "copyfiles -u 1 legacy/*.scss lib",
    "clean": "rm -rf lib es types",
    "build:types": "tsc --rootDir src --declarationDir types --emitDeclarationOnly",
    "build": "yarn build:types && yarn build:cjs && yarn build:es && yarn build:legacy",
    "prepare": " yarn clean && yarn build",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "watch": "yarn build:es --watch"
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.5.5",
    "@datacamp/waffles-axe-render": "^1.1.3",
    "@datacamp/waffles-babel-preset": "^1.2.3",
    "@datacamp/waffles-icons": "^1.4.0",
    "@datacamp/waffles-tsconfig": "^1.1.2",
    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "8.0.7",
    "@testing-library/user-event": "7.0.0",
    "@types/airbnb-prop-types": "2.13.1",
    "@types/jest": "24.0.18",
    "babel-jest": "24.9.0",
    "jest": "24.9.0",
    "jest-emotion": "10.0.17",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.0",
    "@datacamp/waffles-text": "^0.12.3",
    "@datacamp/waffles-tokens": "^0.6.3",
    "@datacamp/waffles-utils": "^1.1.1",
    "@emotion/core": "^10.0.17",
    "airbnb-prop-types": "^2.15.0",
    "lodash": "^4.17.13",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "@datacamp/waffles-icons": "^1.3.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "bugs": {
    "url": "https://github.com/datacamp/design-system/issues"
  },
  "gitHead": "033030174dbf18623ccd9c0174108c41a9e588a1"
}
