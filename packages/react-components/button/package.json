{
  "name": "@datacamp/waffles-button",
  "version": "4.7.0",
  "description": "The Waffles Button",
  "keywords": [
    "button"
  ],
  "author": "Dan Denney <dd.subscriptions@gmail.com>",
  "homepage": "https://github.com/datacamp/design-system/tree/master/packages/button/README.md",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build:cjs": "BABEL_ENV=cjs babel src --extensions '.tsx','.ts' --out-dir lib --ignore \"src/**/*.spec.tsx\",\"src/**/*.spec.ts\" --config-file ./babel7.config.js",
    "build:es": "BABEL_ENV=es babel src --extensions '.tsx','.ts' --out-dir es --ignore \"src/**/*.spec.tsx\",\"src/**/*.spec.ts\" --config-file ./babel7.config.js",
    "build:legacy": "copyfiles -u 1 legacy/*.scss lib",
    "build:metadata": "react-docgen src --exclude \"/^.+spec.+$/\" -x tsx -o componentMetadata.json --resolver findAllComponentDefinitions",
    "clean": "rm -rf lib es types componentMetadata.json",
    "build:types": "tsc --rootDir src --declarationDir types --emitDeclarationOnly",
    "build": "yarn build:types && yarn build:cjs && yarn build:es && yarn build:legacy && yarn build:metadata",
    "prepare": " yarn clean && yarn build",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "watch": "yarn build:es --watch",
    "stryker": "stryker run"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/core": "7.7.7",
    "@datacamp/waffles-axe-render": "^1.2.0",
    "@datacamp/waffles-babel-preset": "^1.3.10",
    "@datacamp/waffles-icons": "^1.8.3",
    "@datacamp/waffles-tsconfig": "^1.1.6",
    "@emotion/core": "^10.0.17",
    "@stryker-mutator/core": "^2.4.0",
    "@stryker-mutator/html-reporter": "^2.4.0",
    "@stryker-mutator/typescript": "^2.4.0",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.4.0",
    "@testing-library/user-event": "7.1.2",
    "@types/airbnb-prop-types": "2.13.1",
    "babel-jest": "24.9.0",
    "jest": "24.9.0",
    "jest-emotion": "10.0.27",
    "react": "16.8.6",
    "react-docgen": "5.1.0",
    "react-dom": "16.8.6",
    "react-error-boundary": "^1.2.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "@datacamp/waffles-card": "^2.2.0",
    "@datacamp/waffles-text": "^2.2.0",
    "@datacamp/waffles-tokens": "^0.6.8",
    "@datacamp/waffles-utils": "^1.3.0",
    "airbnb-prop-types": "^2.15.0",
    "lodash": "^4.17.13",
    "prop-types": "^15.7.2",
    "react-merge-refs": "^1.0.0",
    "react-uid": "^2.2.0",
    "tinycolor2": "^1.4.1",
    "use-callback-ref": "^1.2.1"
  },
  "peerDependencies": {
    "@datacamp/waffles-icons": "^1.3.3",
    "@emotion/core": "^10.0.17",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "bugs": {
    "url": "https://github.com/datacamp/design-system/issues"
  },
  "gitHead": "033030174dbf18623ccd9c0174108c41a9e588a1"
}
