      "greenLight": "#65FF8F",
      "green": "#03EF62",
      "greenDark": "#00C53B",
      "greenDarkText": "#008700"
    },
    "neutral": {
      "white": "#FFFFFF",
      "greySubtle": "#F7F7FC",
      "greyLight": "#EFEFEF",
      "grey": "#E8E8EA",
      "greyMedium": "#D9D9E2",
      "greyDark": "#848C92",
      "beigeSubtle": "#FFFBF3",
      "beigeLight": "#F7F3EB",
      "beige": "#EFEBE4",
      "beigeMedium": "#E5E1DA"
    },
    "accent": {
      "redLight": "#FF782D",
      "red": "#FF5400",
      "redDark": "#DD3400",
      "redDarkText": "#C01100",
      "orangeLight": "#FFBC4B",
      "orange": "#FF931E",
      "orangeDark": "#D87300",
      "orangeDarkText": "#B75900",
      "yellowLight": "#FFEC3C",
      "yellow": "#FCCE0D",
      "yellowDark": "#CFA600",
      "yellowDarkText": "#907000",
      "blueLight": "#60E7FF",
      "blue": "#06BDFC",
      "blueDark": "#009BD8",
      "blueDarkText": "#0075AD",
      "purpleLight": "#974DFF",
      "purple": "#7933FF",
      "purpleDark": "#5646A5",
      "purpleDarkText": "#5646A5",
      "pinkLight": "#FF95CF",
      "pink": "#FF6EA9",
      "pinkDark": "#DC4D8B",
      "pinkDarkText": "#BF3072"
    },
    "brand": {
      "brandFacebook": "#1778F2",
      "brandTwitter": "#00ACEE",
      "brandGoogle": "#DB4437",
      "brandLinkedIn": "#0E76A8"
    }
  },
  "borderRadius": {
    "medium": "4px",
    "circle": "50%"
  },
  "borderWidth": {
    "thin": "1px",
    "medium": "2px",
    "thick": "3px",
    "xthick": "4px"
  },
  "boxShadow": {
    "thin": {
      "value": {
        "x": "0px",
        "y": "0px",
        "spread": "0px",
        "color": "rgba(5, 25, 45, 0.3)",
        "blur": "2px"
      }
    },
    "medium": {
      "value": {
        "x": "0px",
        "y": "1px",
        "spread": "-1px",
        "color": "rgba(5, 25, 45, 0.3)",
        "blur": "4px"
      }
    },
    "thick": {
      "value": {
        "x": "0px",
        "y": "3px",
        "spread": "-1px",
        "color": "rgba(5, 25, 45, 0.3)",
        "blur": "5px"
      }
    },
    "xthick": {
      "value": {
        "x": "0px",
        "y": "8px",
        "spread": "-4px",
        "color": "rgba(5, 25, 45, 0.3)",
        "blur": "12px"
      }
    }
  },
  "fontWeights": {
    "regular": "Regular",
    "bold": "Bold"
  },
  "fontFamilies": {
    "sansSerif": "Studio Feixen Sans",
    "mono": "JetBrains Mono NL"
  },
  "fontSizes": {
    "small": "12px",
    "medium": "14px",
    "large": "16px",
    "xlarge": "18px",
    "xxlarge": "20px",
    "huge": "28px"
  },
  "letterSpacing": {
    "tight": "-0.5px",
    "default": "0px",
    "relaxed": "1.5px"
  },
  "lineHeights": {
    "tight": "100%",
    "default": "125%",
    "relaxed": "150%"
  },
  "opacity": {
    "low": "15%",
    "medium": "40%",
    "high": "60%"
  },
  "zIndex": {
    "default": 1,
    "sticky": 100,
    "dropdown": 7000,
    "overlay": 8000,
    "modal": 9000,
    "toast": 10000
  },
  "breakpoints": {
    "small": "480px",
    "medium": "820px",
    "large": "1480px"
  },
  "paragraphSpacing": {
    "default": "0px"
  },
  "typography": {
    "heading": {
      "h1": {
        "fontFamily": "$fontFamilies.sansSerif",
        "fontWeight": "$fontWeights.bold",
        "lineHeight": "$lineHeights.default",
        "fontSize": "$fontSizes.xxlarge",
        "letterSpacing": "$letterSpacing.default",
        "paragraphSpacing": "$paragraphSpacing.default"
      },
      "h2": {
        "fontFamily": "$fontFamilies.sansSerif",
        "fontWeight": "$fontWeights.bold",
        "lineHeight": "$lineHeights.default",
        "fontSize": "$fontSizes.xlarge",
        "letterSpacing": "$letterSpacing.default",
        "paragraphSpacing": "$paragraphSpacing.default"
      },
      "h3": {
        "fontFamily": "$fontFamilies.sansSerif",
        "fontWeight": "$fontWeights.bold",
